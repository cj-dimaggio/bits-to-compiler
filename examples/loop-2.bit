org 0x7C00

boot:
    mov si hello
    mov ah 0x0E
loop:
    lodsb
    or al al
    0b01110100 ; JZ (jump short relatively)
    0b00000100 ; jump forward 4 bytes if AL is zero
    int 0x10
    0b11101011 ; JMP
    0b11110111 ; 247, backwards 9 bytes
halt:
    cli
    hlt

hello: "Hello, world!"

times 481 0b00000000

0b01010101 ; Boot sector magic
0b10101010
